{
  "name": "webship/cucumber",
  "description": "Cucumber Management System",
  "type": "drupal-profile",
  "license": "GPL-2.0-or-later",
  "homepage": "https://webship.co",
  "support": {
    "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
    "chat": "https://app.slack.com/client/T06GX3JTS/C7MAHMJRE",
    "issues": "http://drupal.org/project/issues/cucumber",
    "source": "http://cgit.drupalcode.org/cucumber"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Webship",
      "homepage": "https://www.drupal.org/webship",
      "role": "Volunteer contributions"
    }
  ],
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    "assets": {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  },
  "require": {
    "composer/installers": "~2.0",
    "oomphinc/composer-installers-extender": "~2.0",
    "cweagans/composer-patches": "~1.0",
    "drupal/core-composer-scaffold": "~9.0",
    "drupal/core": "~9.0",
    "drush/drush": "~11.0",
    "drupal/webconfig": "1.0.x-dev",
    "drupal/webdev": "9.1.x-dev",
    "drupal/webadmin": "9.0.x-dev"
  },
  "require-dev": {
    "drupal/core-dev": "~9.0"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "config": {
    "bin-dir": "bin/",
    "secure-http": false,
    "optimize-autoloader": true,
    "preferred-install": {
      "drupal/core": "dist"
    },
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "oomphinc/composer-installers-extender": true,
      "drupal/core-composer-scaffold": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "drupal-scaffold": {
      "allowed-packages": [
        "drupal/core",
        "webship/cucumber"
      ],
      "gitignore": true,
      "locations": {
        "web-root": "web/"
      }
    },
    "installer-paths": {
      "web/core": ["type:drupal-core"],
      "web/profiles/contrib/{$name}": ["type:drupal-profile"],
      "web/modules/contrib/{$name}": ["type:drupal-module"],
      "web/themes/contrib/{$name}": ["type:drupal-theme"],
      "web/libraries/{$name}": ["type:drupal-library"],
      "web/modules/custom/{$name}": ["type:drupal-custom-module"],
      "web/themes/custom/{$name}": ["type:drupal-custom-theme"],
      "drush/contrib/{$name}": ["type:drupal-drush"]
    },
    "enable-patching": true,
    "composer-exit-on-patch-failure": true,
    "patchLevel": {
      "drupal/core": "-p2"
    },
    "patches": {}
  }
}